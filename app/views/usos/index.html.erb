<h1>Listing usos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usos.each do |uso| %>
      <tr>
        <td><%= uso.nombre %></td>
        <td><%= link_to 'Show', uso %></td>
        <td><%= link_to 'Edit', edit_uso_path(uso) %></td>
        <td><%= link_to 'Destroy', uso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uso', new_uso_path %>
